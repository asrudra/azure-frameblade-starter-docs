{
    "compilerOptions": {
        "allowJs": true,
        "allowUnreachableCode": false,
        "baseUrl": ".",
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": true,
        "importHelpers": true,
        "jsx": "react",
        "lib": [
            "dom",
            "es2015"
        ],
        "module": "commonjs",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "preserveConstEnums": true,
        "removeComments": false,
        "sourceMap": true,
        "target": "ES5"
    },
    "paths": {
        "office-ui-fabric-react/lib*": [
          "office-ui-fabric-react/lib-commonjs*"
        ],
        "@iotHubControlPlane/*": [
            "node_modules/microsoft-azure-devices-iothub-controlplane20180401/*"
        ]
    }
}
